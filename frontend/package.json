{
  "name": "palaceofquests-frontend",
  "version": "1.0.0",
  "private": true,
  "description": "Palace of Quests | Pi Network dApp frontend",
  "author": "Erik G.",
  "license": "PIOS",
  "repository": {
    "type": "git",
    "url": "https://github.com/erikg713/palaceofquests.git",
    "directory": "frontend"
  },
  "bugs": {
    "url": "https://github.com/erikg713/palaceofquests/issues"
  },
  "homepage": "https://github.com/erikg713/palaceofquests#readme",
  "keywords": [
    "pi-network",
    "dapp",
    "web3",
    "frontend",
    "react",
    "vite"
  ],
  "main": "src/main.jsx",
  "files": [
    "dist",
    "src",
    "public",
    "!**/*.test.*"
  ],
  "engines": {
    "node": ">=18.0.0",
    "npm": ">=8.0.0"
  },
  "scripts": {
    "audit": "npm audit --audit-level=moderate",
    "audit:fix": "npm audit fix",
    "build": "vite build",
    "clean": "rimraf dist",
    "dev": "vite",
    "format": "prettier --write \"src/**/*.{js,jsx,css,md}\"",
    "lint": "eslint \"src/**/*.{js,jsx}\" --max-warnings=0",
    "lint:css": "stylelint \"src/**/*.css\"",
    "lint:fix": "eslint \"src/**/*.{js,jsx}\" --fix",
    "predeploy": "npm run build",
    "prepare": "husky install",
    "preview": "vite preview",
    "test": "vitest",
    "test:ui": "vitest --ui",
    "test:coverage": "vitest --coverage"
  },
  "dependencies": {
    "@supabase/supabase-js": "^2.39.0",
    "axios": "^1.6.7",
    "clsx": "^2.1.0",
    "prop-types": "^15.8.1",
    "react": "^18.2.0",
    "react-dnd": "^16.0.1",
    "react-dnd-html5-backend": "^16.0.1",
    "react-dom": "^18.2.0",
    "react-router-dom": "^6.21.3"
  },
  "devDependencies": {
    "@testing-library/jest-dom": "^6.4.2",
    "@testing-library/react": "^14.2.1",
    "@testing-library/user-event": "^14.5.2",
    "@types/react": "^18.2.14",
    "@types/react-dom": "^18.2.7",
    "@vitejs/plugin-react": "^4.2.1",
    "@vitest/coverage-v8": "^1.2.2",
    "@vitest/ui": "^1.2.2",
    "eslint": "^8.57.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-react": "^7.34.0",
    "eslint-plugin-react-hooks": "^4.6.0",
    "husky": "^9.0.10",
    "jsdom": "^24.0.0",
    "lint-staged": "^15.2.2",
    "prettier": "^3.2.5",
    "rimraf": "^5.0.5",
    "stylelint": "^16.2.1",
    "stylelint-config-standard": "^36.0.0",
    "vite": "^5.2.0",
    "vitest": "^1.2.2"
  },
  "lint-staged": {
    "*.{js,jsx}": [
      "eslint --fix",
      "prettier --write"
    ],
    "*.css": [
      "stylelint --fix",
      "prettier --write"
    ],
    "*.{md,json}": [
      "prettier --write"
    ]
  }
}
