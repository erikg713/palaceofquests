{
  "name": "palaceofquests-frontend",
  "version": "1.0.0",
  "private": true,
  "description": "A polished, professional Pi Network dApp frontend for the Palace of Quests metaverse.",
  "author": "Erik G.",
  "license": "SEE LICENSE IN LICENSE",
  "repository": {
    "type": "git",
    "url": "https://github.com/erikg713/palaceofquests.git",
    "directory": "frontend"
  },
  "bugs": {
    "url": "https://github.com/erikg713/palaceofquests/issues"
  },
  "homepage": "https://github.com/erikg713/palaceofquests#readme",
  "keywords": [
    "pi-network",
    "dapp",
    "web3",
    "frontend",
    "react",
    "vite",
    "typescript"
  ],
  "scripts": {
    "dev": "vite",
    "build": "vite build",
    "preview": "vite preview",
    "type-check": "tsc --noEmit",
    "lint": "eslint \"src/**/*.{ts,tsx,js,jsx}\" --max-warnings=0",
    "format": "prettier --write \"src/**/*.{ts,tsx,js,jsx,css,md,json}\"",
    "clean": "rimraf dist",
    "test": "vitest run",
    "prepare": "husky install",
    "audit": "npm audit --audit-level=moderate"
  },
  "dependencies": {
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "@pi-network/pi-sdk": "2.0.0"
  },
  "devDependencies": {
    "@types/react": "18.2.14",
    "@types/react-dom": "18.2.7",
    "typescript": "5.4.0",
    "eslint": "8.57.0",
    "eslint-config-prettier": "9.1.0",
    "eslint-plugin-react": "7.34.0",
    "eslint-plugin-react-hooks": "4.6.0",
    "eslint-plugin-import": "2.28.1",
    "prettier": "3.2.5",
    "vite": "5.4.19",
    "rimraf": "5.0.5",
    "vitest": "1.6.1",
    "husky": "9.0.0"
  },
  "engines": {
    "node": ">=18.0.0"
  },
  "browserslist": [
    ">0.2%",
    "not dead",
    "not op_mini all"
  ]
}
